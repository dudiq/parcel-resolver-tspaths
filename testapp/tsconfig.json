{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react",
    "newLine": "lf",
    "baseUrl": "notSrc",
    "paths": {
      "@c": [
        "a/b/c/index"
      ],
      "@c/*": [
        "a/b/c/*",
        "a/b/c/index"
      ],
      "@b": [
        "a/b/index"
      ],
      "@b/*": [
        "a/b/*",
        "a/b/index"
      ],
      "@a": [
        "a/index"
      ],
      "@a/*": [
        "a/*",
        "a/index"
      ],
      "@d": [
        "d/index"
      ],
      "@d/*": [
        "d/*",
        "d/index"
      ],
      "@/*": [
        "./*"
      ],
      "~/*": [
        "../*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "**/lib",
    "**/dist",
    "node_modules"
  ]
}